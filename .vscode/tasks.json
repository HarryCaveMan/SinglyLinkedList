{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Build with c++-10",
			"command": "/usr/bin/g++-10",
			"args": [
				"-g",
				"-std=c++20",
				"${workspaceFolder}/main.cpp",
				"-o",
				"${workspaceFolder}/LinkedListTest"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/g++-10"
		}
	]
}